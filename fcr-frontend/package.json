    "font-awesome": "^4.7.0",
    "npm-check-updates": "^16.10.12",
    "pinia": "^2.0.36",
    "primeicons": "^6.0.1",
    "primevue": "^3.29.1",
    "vue": "^3.3.2",
    "vue-router": "^4.2.0"
  },
  "devDependencies": {
